# Copy this file to .env and fill in your secrets. Do NOT commit .env.

# Google OAuth (GIS)
VITE_GOOGLE_CLIENT_ID=

# Veo/Gemini API (video generation)
VITE_VEO_API_KEY=
VITE_VEO_API_ENDPOINT=https://generativelanguage.googleapis.com
# Common create path for Veo (auto-fallbacks are implemented in code)
VITE_VEO_CREATE_PATH=/v1beta/models/veo-3.0-generate-preview:predictLongRunning
# Optional tuning
VITE_VEO_POLL_INTERVAL_MS=10000
VITE_VEO_TIMEOUT_MS=180000

# Gemini Image (optional)
VITE_GEMINI_API_KEY=
VITE_GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com
VITE_GEMINI_IMAGE_PATH=/v1beta/models/gemini-2.5-flash-image-preview:generateContent

# Firebase (if used)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# App behavior
VITE_VEO_USE_MOCK=false
VITE_APP_NAME=Katha AI
VITE_APP_URL=http://localhost:3000
